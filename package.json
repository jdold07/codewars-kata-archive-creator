    "@types/json2md": "^1.5.1",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.7.23",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "eslint": "^8.24.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
