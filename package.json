  "bugs": {
    "url": "https://github.com/jdold07/kata2markdown/issues",
    "email": "kata2markdown@tpstech.dev"
  },
  "main": "./dist/app.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "k2m": "node ./dist/app.js"
  },
  "keywords": [
    "Codewars",
    "Kata Solution",
    "Markdown",
    "json2md"
